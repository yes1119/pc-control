<Controls:MetroWindow x:Class="Admin.Dialog_Get.PcTimerDel"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"           
                      xmlns:System="clr-namespace:System;assembly=mscorlib"
                      ResizeMode="NoResize" 
                      WindowStyle="None"  
                      Title="Pc Timer Delete"
                      WindowStartupLocation="CenterScreen" 
                      Height="250"
                      Width="300"
                      ShowMaxRestoreButton="False" TitleForeground="#999988">
    <Grid>
        <CheckBox x:Name="check1" Content="Pc On Timer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,27,0,0" Foreground="White" Checked="checkon_Checked"/>
        <CheckBox x:Name="check2" Content="Pc Off Timer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,86,0,0" Foreground="White" Checked="checkon_Checked"/>
        <Button Content="삭제" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="96" Height="64" Click="Button_Click" Margin="0,0,23,23"/>

    </Grid>
</Controls:MetroWindow>
